{
  "root": true,
  "env": {
    "node": true
  },
  "plugins": ["unused-imports"],
  "extends": [
    "plugin:vue/vue3-recommended",
    "plugin:prettier/recommended",
    "eslint:recommended",
    "@vue/typescript/recommended"
  ],
  "rules": {
    "no-undef": "off",
    "vue/multi-word-component-names": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
